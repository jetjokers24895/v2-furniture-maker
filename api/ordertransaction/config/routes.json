{
  "routes": [
    {
      "method": "GET",
      "path": "/ordertransaction",
      "handler": "Ordertransaction.find",
      "config": {
        "policies": ["global.isOwner"]
      }
    },
    {
      "method": "GET",
      "path": "/ordertransaction/count",
      "handler": "Ordertransaction.count",
      "config": {
        "policies": ["global.isOwner"]
      }
    },
    {
      "method": "GET",
      "path": "/ordertransaction/:_id",
      "handler": "Ordertransaction.findOne",
      "config": {
        "policies": ["global.isOwner"]
      }
    },
    {
      "method": "POST",
      "path": "/ordertransaction",
      "handler": "Ordertransaction.create",
      "config": {
        "policies": ["global.setOwner"]
      }
    },
    {
      "method": "PUT",
      "path": "/ordertransaction/:_id",
      "handler": "Ordertransaction.update",
      "config": {
        "policies": ["global.isOwner"]
      }
    },
    {
      "method": "PUT",
      "path": "/ordertransaction/confirm/:_id",
      "handler": "Ordertransaction.confirm",
      "config": {
        "policies": ["global.isOwner"]
      }
    },
    {
      "method": "DELETE",
      "path": "/ordertransaction/:_id",
      "handler": "Ordertransaction.destroy",
      "config": {
        "policies": ["global.isOwner"]
      }
    }
  ]
}