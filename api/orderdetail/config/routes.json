{
  "routes": [
    {
      "method": "GET",
      "path": "/orderdetail",
      "handler": "Orderdetail.find",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/orderdetail/count",
      "handler": "Orderdetail.count",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/orderdetail/:_id",
      "handler": "Orderdetail.findOne",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "POST",
      "path": "/orderdetail",
      "handler": "Orderdetail.create",
      "config": {
        "policies": [
          "global.setOwner"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/orderdetail/:_id",
      "handler": "Orderdetail.update",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/orderdetail/update-quantity/:_id",
      "handler": "Orderdetail.updateQuantity",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "DELETE",
      "path": "/orderdetail/:_id",
      "handler": "Orderdetail.destroy",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    }
  ]
}
