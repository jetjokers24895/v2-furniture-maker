{
  "connection": "default",
  "collectionName": "businesslicense",
  "info": {
    "name": "businesslicense",
    "description": "Business license"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "companyName": {
      "default": "",
      "unique": true,
      "type": "string"
    },
    "companyAddress": {
      "default": "",
      "type": "string"
    },
    "businessAreas": {
      "default": "",
      "type": "string"
    },
    "businessLicense": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "created_by": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "isBusiness": {
      "default": false,
      "type": "boolean"
    },
    "status": {
      "default": "pending",
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected"
      ]
    },
    "agency": {
      "model": "agency",
      "via": "businessLicense"
    }
  }
}