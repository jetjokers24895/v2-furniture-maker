{
  "routes": [
    {
      "method": "GET",
      "path": "/businesslicenses",
      "handler": "Businesslicense.find",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/businesslicenses/count",
      "handler": "Businesslicense.count",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/businesslicenses/:_id",
      "handler": "Businesslicense.findOne",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "POST",
      "path": "/businesslicenses",
      "handler": "Businesslicense.create",
      "config": {
        "policies": [
          "global.setOwner"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/businesslicenses/:_id",
      "handler": "Businesslicense.update",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    },
    {
      "method": "DELETE",
      "path": "/businesslicenses/:_id",
      "handler": "Businesslicense.destroy",
      "config": {
        "policies": [
          "global.isOwner"
        ]
      }
    }
  ]
}