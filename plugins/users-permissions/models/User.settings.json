{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "agency": {
      "model": "agency",
      "via": "user"
    },
    "dayOfBirth": {
      "default": "",
      "type": "integer"
    },
    "monthOfBirth": {
      "default": "",
      "type": "integer"
    },
    "yearOfBirth": {
      "default": "",
      "type": "integer"
    },
    "phone": {
      "default": "",
      "type": "string"
    },
    "gender": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "male",
        "famale"
      ]
    },
    "fullName": {
      "default": "",
      "type": "string"
    },
    "accountrequest": {
      "model": "accountrequest",
      "via": "createdBy"
    }
  },
  "collectionName": "users-permissions_user"
}